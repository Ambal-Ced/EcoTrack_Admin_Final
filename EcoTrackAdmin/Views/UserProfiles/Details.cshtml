@model EcoTrackAdmin.Models.UserProfile

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>UserProfile</h4>
    <hr />
    <dl class="row">
        <dd class = "col-sm-10">
            <h6>ID: 
            @Html.DisplayFor(model => model.ApplicationUser.Id)
            </h6>
        </dd>
        <dd class="col-sm-10">
            <h6>First Name:
                @Html.DisplayFor(model => model.ApplicationUser.FirstName)
            </h6>
        </dd>
        <dd class="col-sm-10">
            <h6> Last Name: 
            @Html.DisplayFor(model => model.ApplicationUser.LastName)
            </h6>
        </dd>
        <dd class="col-sm-10">
            <h6>Email:
                @Html.DisplayFor(model => model.ApplicationUser.Email)
            </h6>
        </dd>
        <dd class="col-sm-10">
            <h6>Type: 
            @Html.DisplayFor(model => model.ApplicationUser.Type)
            </h6>
        </dd>
        <dd class="col-sm-10">
            <h6>Contact No.:
                @Html.DisplayFor(model => model.ApplicationUser.PhoneNumber)
            </h6>
        </dd>

        <dd class="col-sm-10">
            <h6>Username:
                @Html.DisplayFor(model => model.UniqueUserName)
            </h6>
        </dd>
        <dd class="col-sm-10">
            <h6>Date Created:
                @Html.DisplayFor(model => model.Month)
            </h6>
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.UniqueUserName">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
